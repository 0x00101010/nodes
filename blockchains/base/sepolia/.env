# shared env vars
COMPOSE_PROJECT_NAME=base-sepolia
NETWORK=sepolia
CL_DNS="base-sepolia-node"
EL_DNS="base-sepolia-reth"
PROMETHEUS_DNS="base-sepolia-prometheus"

L1_BEACON_DNS=
L1_ETH_DNS=

# reth env vars
RETH_BIN=op-reth
RETH_CHAIN=base-sepolia
RETH_GCMODE=full
RETH_SEQUENCER_HTTP=https://sepolia-sequencer.base.org
RETH_LOG_LEVEL=debug
RETH_ALLOWED_APIS=web3,debug,eth,txpool,net,admin,rpc
RETH_FLASHBLOCKS_URL=wss://sepolia.flashblocks.base.org/ws

# -------------------------------------
# Build Profile Configuration
# -------------------------------------
# Build configuration is now controlled via docker-compose override files
# Usage:
#   ./up.sh prebuilt  - Use prebuilt binary from GitHub (fastest)
#   ./up.sh dev       - Build from source with dev profile (fast compile, debug symbols)
#   ./up.sh release   - Build from source with release profile (optimized, slow compile)
#
# Variables used by different profiles:
# - prebuilt: Uses RETH_VERSION
# - dev/release: Uses RETH_REPO, RETH_COMMIT, and RUST_VERSION

# For prebuilt profile
RETH_VERSION=v1.9.0

# For dev and release profiles
RETH_REPO=https://github.com/0x00101010/reth.git
RETH_COMMIT=7f31cbbcb93539e3b9cc46724a616b0d72c485d3 # flashblock engine sidecar
RUST_VERSION=1.88
# -------------------------------------
# End of Build Profile Configuration
# -------------------------------------

EL_RPC_PORT=6000
EL_WS_PORT=6001
EL_AUTHRPC_PORT=6002
EL_METRICS_PORT=6003
EL_P2P_PORT=30305

# op-node env vars
OP_NODE_NETWORK=base-sepolia
OP_NODE_LOG_LEVEL=info
OP_NODE_L1_TRUST_RPC=true
OP_NODE_L1_ETH_RPC=${L1_ETH_DNS}
OP_NODE_L1_BEACON=${L1_BEACON_DNS}
OP_NODE_L2_ENGINE_AUTH=/config/jwtsecret
OP_NODE_L2_ENGINE_RPC="http://base-sepolia-reth:${EL_AUTHRPC_PORT}" # based on reth configuration
OP_NODE_L2_ENGINE_KIND=reth
OP_NODE_RPC_ADDR=0.0.0.0
OP_NODE_RPC_PORT=6020
OP_NODE_VERIFIER_L1_CONFS=4
OP_NODE_ROLLUP_LOAD_PROTOCOL_VERSIONS=true
OP_NODE_SYNCMODE=execution-layer

